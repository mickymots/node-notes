{
  "name": "@itmp/arch",
  "version": "2.5.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.js",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check --force && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check --force",
    "lintsrc": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check --force -t pmd --out src.pmd.xml",
    "linttests": "tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check --force -t pmd --out e2e.pmd.xml",
    "test": "ng test",
    "karma": "ng test --watch false --browsers PhantomJS --code-coverage",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "high-contrast": "node high-contrast/concat && npm run sass && postcss -c high-contrast/postcss.config.json && del-cli high-contrast/temp",
    "sass": "node-sass --output-style compressed -o high-contrast/temp high-contrast/temp --include-path=node_modules/@itmp/ui/src --include-path=src/styles --include-path=node_modules/bootstrap-sass/assets/stylesheets",
    "build": "ng build --base-href /ITMP/",
    "buildprod": "ng build --base-href /ITMP/ --prod",
    "retire": "retire --path . --outputpath ./reports/test_results --exitwith 0"
  },
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.3",
    "@ngrx/store-devtools": "3.2.4",
    "core-js": "2.5.3",
    "intl": "1.2.5",
    "moment": "2.20.1",
    "ngx-progressbar": "2.1.1",
    "rxjs": "5.5.2",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "4.2.4",
    "@angular/language-service": "4.2.5",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.52",
    "codelyzer": "3.0.1",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-html-detailed-reporter": "^1.1.4",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-xvfb-chrome-launcher": "0.0.1",
    "npm": "5.6.0",
    "protractor": "5.1.2",
    "ts-node": "3.0.6",
    "tslint": "5.3.2",
    "typescript": "2.3.4"
  }
}
